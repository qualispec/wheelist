<title>wheelist - Sign In</title>
<h1>Sign In</h1>

<div class="container">
	<div>
		<%= simple_form_for(:session, url: sessions_path,
				html: { class: 'form-horizontal' } ) do |f| %>
			<%= f.input :email %>
			<%= f.input :password %>

			<%= f.submit "Sign In", class: "btn btn-primary btn-large" %>
		<% end %>
	</div>
</div>